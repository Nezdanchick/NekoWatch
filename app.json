{
  "expo": {
    "name": "Neko Watch",
    "slug": "neko-watch",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/title.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ru.nekoteam.nekowatch"
    },
    "android": {
      "package": "ru.nekoteam.nekowatch",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "overlay-swipe",
          "backgroundColor": "#00000080"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "expo-navigation-bar": {
        "position": "absolute",
        "visibility": "hidden",
        "behavior": "overlay-swipe",
        "backgroundColor": "#00000080"
      },
      "eas": {
        "projectId": "fbc9e325-901d-414f-a91b-34702b7ffca9"
      }
    },
    "owner": "nekoteam"
  }
}
